<div class="container">
	<div class="row">
		<div class="col-xs-12">
			<h2>通知一覧</h2>
			<% if @notifications.exists? %>
			    <%= render @notifications %>
			    <div class="notification_pagenate">
				    <%= will_paginate @notifications, :previous_label => ' &lt 前へ', :next_label => '次へ &gt' %>
				</div>
			<% else %>
			    <p>通知はありません</p>
			<% end %>
		</div>
	</div>
</div>